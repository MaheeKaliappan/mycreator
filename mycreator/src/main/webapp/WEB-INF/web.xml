<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>mycreator</display-name>
	<welcome-file-list>
		<welcome-file>views/jsp/addnewuser.jsp</welcome-file>
	</welcome-file-list>


	<servlet>
		<servlet-name>StudentController</servlet-name>
		<servlet-class>mycreator.controller.StudentController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StudentController</servlet-name>
		<url-pattern>/addnewuser</url-pattern>
		<url-pattern>/studentdetails</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AdminController</servlet-name>
		<servlet-class>mycreator.controller.AdminController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AdminController</servlet-name>
		<url-pattern>/departement</url-pattern>
		<url-pattern>/branch</url-pattern>
		<url-pattern>/batchperiod</url-pattern>
		<url-pattern>/compliationstatus</url-pattern>

	</servlet-mapping>
















	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>mycreator.controller.LoginController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	
</web-app>